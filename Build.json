{
    "markdown_dir": "markdown",

    "site_dir": "docs",

    "HTML_template": "Template",

    "Exclude": [
    ],

    "markdown_extensions": [
        "common",
        "definitionlists",
        "mathematics",
        "diagrams",
        "pipetables",
        "autoidentifiers",
        "attributes",
        "emojis",
        "emphasisextras"
    ],

    "github_pages": true,

    "footer": "Created by PowerShell Module <a href='https://wethat.github.io/MarkdownToHtml/'>MarkdownToHTML</a>.",

    "navigation_bar": {
        "capture_page_headings": "123456",
        "templates": {
            "navitem": "<button class='navitem'><a href='{{navurl}}'>{{navtext}}</a></button>",
            "navlabel": "<div class='navlabel'>{{navtext}}</div>",
            "navseparator": "<hr/>",
            "navheading": "<span class='navitem{{level}}'>{{navtext}}</span>"
        }
    },

    "site_navigation": [
        { "<div style='text-align:center;'><img src='images/TaggingKitRibbon.png' /></div>": "index.md" },
        { "Home": "index.md" }
    ],

    "page_navigation_header": [
        { "On this Page": "" }
    ],

    "svgbob": {
        "background": "white",
        "fill_color": "black",
        "font_size": 14,
        "font_family": "Monospace",
        "scale": 1,
        "stroke_width": 2
    }
}