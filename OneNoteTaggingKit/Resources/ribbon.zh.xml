<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" loadImage="GetImage">
  <ribbon>
    <tabs>
      <tab idMso="TabHome">
        <group id="PageTagging" label="页面标签" insertAfterMso="GroupTagging">
          <button id="buttonTagPage"
                  label="标签页面"
                  size="large"
                  screentip="在OneNote页面上添加标签或从中删除标签。"
                  onAction="editTags"
                  image="pageTags.png" />
            <button id="updatePage"
                     label="更新頁面"
                     size="normal"
                     screentip="更新当前页面上导入的标签和保存的标签搜索"
                     onAction="updatePage"
                     image="refreshPage.png" />
          <button id="buttonSearchTags"
                  label="查找页面"
                  size="normal"
                  screentip="通过标签和内容查找页面"
                  onAction="findTags"
                  image="findPageTags.png" />
          <button id="buttonManageTags"
                  label="设置"
                  size="normal"
                  screentip="管理加载项设置"
                  onAction="manageTags"
                  image="managePageTags.png" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>